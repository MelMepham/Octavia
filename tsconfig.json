{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./projects/octavia",
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./projects/octavia/dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "paths": {
    "@octavia": [
      "packages/octavia/dist/octavia"
    ]
  }
}
