{
  "$schema": "node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ocatvia": {
      "root": "packages/ocatvia",
      "sourceRoot": "packages/ocatvia/src",
      "projectType": "application",
      "prefix": "ocatvia",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ocatvia",
            "index": "packages/ocatvia/src/index.html",
            "main": "packages/ocatvia/src/main.ts",
            "polyfills": "packages/ocatvia/src/polyfills.ts",
            "tsConfig": "packages/ocatvia/src/tsconfig.app.json",
            "assets": [
              "packages/ocatvia/src/favicon.ico",
              "packages/ocatvia/src/assets"
            ],
            "styles": [
              "packages/ocatvia/src/styles.css"
            ],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/ocatvia/src/environments/environment.ts",
                  "with": "packages/ocatvia/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ocatvia:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ocatvia:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ocatvia:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/ocatvia/src/test.ts",
            "polyfills": "packages/ocatvia/src/polyfills.ts",
            "tsConfig": "packages/ocatvia/src/tsconfig.spec.json",
            "karmaConfig": "packages/ocatvia/src/karma.conf.js",
            "styles": [
              "packages/ocatvia/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "packages/ocatvia/src/favicon.ico",
              "packages/ocatvia/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/ocatvia/src/tsconfig.app.json",
              "packages/ocatvia/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "web": {
      "root": "packages/web",
      "sourceRoot": "packages/web/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "scripts": [],
            "outputPath": "dist/web",
            "index": "packages/web/src/index.html",
            "main": "packages/web/src/main.ts",
            "polyfills": "packages/web/src/polyfills.ts",
            "tsConfig": "packages/web/src/tsconfig.app.json",
            "assets": [
              "packages/web/src/favicon.ico",
              "packages/web/src/assets"
            ],
            "styles": [
              "packages/web/src/styles.css"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/web/src/environments/environment.ts",
                  "with": "packages/web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/web/src/test.ts",
            "polyfills": "packages/web/src/polyfills.ts",
            "tsConfig": "packages/web/src/tsconfig.spec.json",
            "karmaConfig": "packages/web/src/karma.conf.js",
            "styles": [
              "packages/web/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "packages/web/src/favicon.ico",
              "packages/web/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/web/src/tsconfig.app.json",
              "packages/web/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "web"
}
